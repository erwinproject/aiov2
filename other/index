	<style>
		body {
			padding: 25px;
			background-color: white;
			color: black;
			font-size: 25px;
		}

		.dark-mode {
			background-color: black;
			color: white;
		}
	</style>
</head>

<body>
	<center>
		<h1>Cahaya Integrasi</h1>
		<h2>Auto script VPS</h2>
		<div>
			<button onclick="darkMode()">Darkmode</button>
		</div>
		<script>
			function darkMode() {
				var element = document.body;
				element.classList.toggle("dark-mode");
			}
		</script>
	</center>
</body>

